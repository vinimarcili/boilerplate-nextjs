{
  "name": "boilerplate-next",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "lint:fix": "eslint src --fix",
    "test": "jest",
    "test:bail": "jest --bail",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -s ./public -p 6006",
    "storybook:build": "build-storybook -s ./public",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "postinstallhusk": "INIT_CWD=\"$PWD\" npm_config_user_agent=$(yarn config get user-agent) node node_modules/husky/husky.js install"
  },
  "dependencies": {
    "next": "10.0.6",
    "next-pwa": "^5.0.4",
    "plop": "^2.7.4",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "styled-components": "^5.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix && npm run lint",
      "pre-push": "npm run test:bail"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@storybook/addon-essentials": "^6.1.15",
    "@storybook/react": "^6.2.2",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "typescript": "^4.1.3"
  }
}
